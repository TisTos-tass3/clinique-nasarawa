{% extends 'base.html.twig' %}

{% block title %}Antecedent{% endblock %}

{% block body %}
    <h1>Antecedent</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ antecedent.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ antecedent.type }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ antecedent.description }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ antecedent.createdAt ? antecedent.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ModifiedAt</th>
                <td>{{ antecedent.modifiedAt ? antecedent.modifiedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_antecedent_index') }}">back to list</a>

    <a href="{{ path('app_antecedent_edit', {'id': antecedent.id}) }}">edit</a>

    {{ include('antecedent/_delete_form.html.twig') }}
{% endblock %}
