{% extends 'base.html.twig' %}

{% block title %}DossierMedical index{% endblock %}

{% block body %}
    <h1>DossierMedical index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>NumeroDossier</th>
                <th>Observations</th>
                <th>CreatedAt</th>
                <th>ModifiedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for dossier_medical in dossier_medicals %}
            <tr>
                <td>{{ dossier_medical.id }}</td>
                <td>{{ dossier_medical.numeroDossier }}</td>
                <td>{{ dossier_medical.observations }}</td>
                <td>{{ dossier_medical.createdAt ? dossier_medical.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ dossier_medical.modifiedAt ? dossier_medical.modifiedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_dossier_medical_show', {'id': dossier_medical.id}) }}">show</a>
                    <a href="{{ path('app_dossier_medical_edit', {'id': dossier_medical.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_dossier_medical_new') }}">Create new</a>
{% endblock %}
